    <!-- Folder where production build objects will be placed -->
    <BuildOutputFolder>$(MSBuildProjectDirectory)\dist\teste-tecnico2.client\browser\</BuildOutputFolder>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="src\app\Componentes\" />
    <Folder Include="src\app\Interfaces\" />
    <Folder Include="src\app\Core\" />
  </ItemGroup>
</Project>